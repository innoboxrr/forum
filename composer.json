{
    "name": "innoboxrr/forum",
    "license": "MIT",
    "type": "library",
    "description": "A simple forum package for your laravel app",
    "authors": [
        {
            "name": "Homero Raul",
            "email": "homero.vargas@innoboxrr.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/cache": "^11.0",
        "illuminate/console": "^11.0",
        "mews/purifier": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
	    "psr-4": {
	        "Innoboxrr\\Forum\\": "/src"
		}
	},
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Innoboxrr\\Forum\\ForumServiceProvider"
            ]
        }
    }
}
